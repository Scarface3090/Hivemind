{
  "name": "hivemind-game",
  "description": "Hivemind - A social guessing game for Reddit communities built with React and Devvit",
  "version": "1.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-react-ts.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-react-ts/issues"
  },
  "homepage": "https://github.com/phaserjs/template-react-ts#readme",
  "keywords": [
    "react",
    "vite", 
    "typescript",
    "game",
    "reddit",
    "devvit"
  ],
  "scripts": {
    "build:client": "vite build",
    "build:server": "vite build --config src/server/vite.config.ts",
    "build": "npm run build:client && npm run build:server",
    "dev": "concurrently -n client,server -c cyan,magenta \"vite\" \"tsup src/server/index.ts --format cjs --out-dir dist/server --out-extension cjs --target node18 --watch --clean --dts=false\""
  },
  "dependencies": {
    "@hiogawa/vite-plugin-error-overlay": "^0.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.28.1",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.26.1",
    "tsup": "^8.1.0",
    "concurrently": "^9.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "vite": "^5.4.0",
    "@anthropic-ai/claude-code": "^1.0.98",
    "@anthropic-ai/sdk": "^0.60.0"
  }
}
